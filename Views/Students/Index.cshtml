@model List<WebAppStudents.Models.Students>

<div>
    <h1>Students</h1>
</div>

<div>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Faculty</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @{
                foreach (var user in Model)
                {
                    <tr>
                        <td>@s</td>
                        <td>@user.Name</td>
                        <td>@user.Email</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>